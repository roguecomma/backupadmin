<!DOCTYPE html>
<html>
<head>
  <title>Backupadmin</title>
  <%= stylesheet_link_tag 'application' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div class="content">
  <div id="flash">
  <% if flash[:alert] %>
    <p class="alert"><%=h flash[:alert]%></p>
  <% end %>
  <% if flash[:notice] %>
    <p class="notice"><%=h flash[:notice]%></p>
  <% end %>
  </div>
  <%= yield %>
</div>

</body>
</html>
